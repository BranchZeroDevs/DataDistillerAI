# OpenAI API Configuration
OPENAI_API_KEY=your_api_key_here

# Vector Database Configuration
VECTOR_DB_PATH=./data/vector_store

# LLM Parameters
LLM_MODEL=gpt-3.5-turbo
LLM_TEMPERATURE=0.7
LLM_MAX_TOKENS=500

# Embedding Configuration
